Proceso sin_titulo
	//Elabore un algoritmo que permita ingresar un número entero (1 a 10), y muestre su 
	//equivalente en romano.
	Definir n Como Entero;
	Escribir 'Ingrese n: ';
	Leer n;
	Segun n Hacer
		1:
			Escribir 'I';
		2:
			Escribir 'II';
		3:
			Escribir 'III';
		4:
			Escribir 'IV';
		5:
			Escribir 'V';
		6:
			Escribir 'VI';
		7:
			Escribir 'VII';
		8:
			Escribir 'VIII';
		9:
			Escribir 'IX';
		10:
			Escribir 'X';
		De Otro Modo:
			Escribir 'ERROR';
	FinSegun
FinProceso
